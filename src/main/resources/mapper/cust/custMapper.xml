<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.hunter.dao.cust.CustDao">

    <select id="selectByPrimaryKey" resultType="com.hunter.entity.cust.Money" parameterType="java.lang.Integer">
        SELECT id, basic, reward, punishment FROM money where id = #{1}
    </select>

</mapper>
