<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.hunter.dao.order.OrderDao">

    <select id="selectByPrimaryKey" resultType="com.hunter.entity.order.User" parameterType="java.lang.Integer">
        SELECT id, name, age, money FROM user where id = #{id}
    </select>

</mapper>
